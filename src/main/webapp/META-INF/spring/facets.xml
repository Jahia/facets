<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="choiceListFacetsInitializer" class="org.jahia.modules.facets.initializers.FacetsChoiceListInitializers">
        <property name="key" value="choicelistfacets"/>
    </bean>

    <bean id="choiceListHierarchicalFacetsInitializer" class="org.jahia.modules.facets.initializers.HierarchicalFacetsChoiceListInitializer">
        <property name="key" value="choicelisthierarchicalfacets"/>
    </bean>
</beans>